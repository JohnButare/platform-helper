@echo off
SetLocal

set program=

iff %@IsKey[] == 1 then
	set program=%@BatchDrive[]documents\Diagnostic
elseiff IsFile "%PublicDocuments\programs\dig\dig.exe" then
	set program=%PublicDocuments\programs\dig\dig.exe
elseiff IsFile "\\oversoul\public\documents\programs\dig\dig.exe" then
	set program=\\oversoul\public\documents\programs\dig\dig.exe
else
	EchoErr dig could not be located.
	quit 1
endiff
	
"%program" %$
quit %?
