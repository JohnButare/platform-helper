@echo off
SetLocal
on break cancel 1

REM Initialize

REM Arguments
if %@IsHelpArg[%@UnQuote[%1]] == 1 goto usage

set command=start
iff IsLabel %1 then
	set command=%1
	shift
endiff

REM Run the command
gosub %command
quit %_?

:usage
text 1>&2
usage: <template> [command1|command2](command1)
endtext
quit 2

:command1
return
