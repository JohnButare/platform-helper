#!/bin/bash
. function.sh

run()
{
	init $*
	${1}Command ${@:2}
	exit "$?"
}

init()
{
	{ [[ $# == 0 ]] || ! IsFunction ${1}Command; } && usage
}

usage()
{
	echot "\
usage: template one|two"
	exit
}

oneCommand()
{
	echo "Command 1"
	pause
}

twoCommand()
{
	echo "Command 2"
	pause
}

run $*

{ [[ $# == 0 ]] || ! IsFunction ${1}Command; } && Usage
${1}Command ${@:2}
exit $?