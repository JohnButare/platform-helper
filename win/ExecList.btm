@echo off
SetLocal

set command=%$

setarray result[50] >& nul:

REM Execute the command and put the results into the array
echo %@ExecArray[result,%$] >& nul:

REM Merge each line of output from the command to a single line 
for /l %i in (0,1, %@eval[%_ExecArray - 1] ) echos %@quote[%result[%i]]` `
