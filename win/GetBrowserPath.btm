@echo off

REM Chrome
iff "%@search[chrome.exe]" != "" then
	echo %@search[chrome.exe]
	quit 0
endiff

REM Find Firefox
iff "%@search[firefox.exe]" != "" then
	echo %@search[firefox.exe]
	quit 0
endiff

iff exist "%programs\Internet Explorer\iexplore.exe" then
	echo %programs\Internet Explorer\iexplore.exe
	quit 0
endiff

REM Find Internet Explorer in the path
iff "%@search[iexplore]" != "" then
	echo %@search[iexplore]
	quit 0
endiff

quit 1

