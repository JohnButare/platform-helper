@echo off
SetLocal
echo %1
REM Initialize

REM Get arguments
if %@IsHelpArg[%@UnQuote[%1]] == 1 goto usage

set command=start
iff %# gt 0 .and. IsLabel %1 then
	set command=%1
	shift
endiff
if not IsLabel %command goto usage

REM Run command
gosub %command
quit %_?

:usage
text 1>&2
usage: DiskSpaceUsage [start](start) [<path>]
endtext
quit 1

:start
echo start /pgm "%@BatchDir[]\windirstat.exe" %@if[ %# == 0 ,.,%$]
return


